{
  "mappings": [
    {
      "request": {
        "urlPath": "/api/partner-token/post-info",
        "method": "POST",
        "headers": {
          "Accept": {
            "contains": ".*"
          }
        },
        "bodyPatterns": [{
          "matchesJsonPath": "$.info"
        }]
      },
      "response": {
        "status": 200,
        "bodyFilePath": "mock-integration/dia-post_info-response.json",
        "headers": {
          "Content-Type": "text/xml"
        },
        "transformers": [
          "response-template",
          "body-transformer"
        ]
      }
    }
  ]
}